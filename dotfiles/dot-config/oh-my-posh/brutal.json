{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "foreground": "darkGray",
          "type": "session"
        },
        {
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": "{{.Path}}",
          "type": "path"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "darkGray",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "darkGray",
          "type": "project"
        },
        {
          "style": "plain",
          "foreground": "green",
          "template": " \uE606 {{.Venv}} ",
          "type": "python",
          "properties": {
            "fetch_virtual_env": true
          },
          "enabled": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "$",
          "foreground": "white",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
